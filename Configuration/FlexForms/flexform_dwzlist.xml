<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<switchableControllerActions>
						<TCEforms>
							<label>Mode</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<items type="array">
									<numindex index="1" type="array">
										<numindex index="0">List-Mode</numindex>
										<numindex index="1">Dwzlist->list;Dwzlist->show</numindex>
									</numindex>
									<numindex index="2" type="array">
										<numindex index="0">Single-Mode</numindex>
										<numindex index="1">Dwzlist->show</numindex>
									</numindex>
								</items>
							</config>
						</TCEforms>
					</switchableControllerActions>
					<settings.zps>
						<TCEforms>
							<label>ZPS</label>
							<config>
								<type>input</type>
								<size>20</size>
								<eval>trim,required</eval>
							</config>
						</TCEforms>
					</settings.zps>
				</el>
			</ROOT>
		</sDEF>
		<graph>
			<ROOT>
				<TCEforms>
					<sheetTitle>Graph</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.hideGraph>
						<TCEforms>
							<label>Hide graph</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.hideGraph>
					<settings.graphColor>
						<TCEforms>
							<label>Graph color</label>
							<config>
								<type>input</type>
								<size>6</size>
								<wizards>
									<color>
										<type>colorbox</type>
										<dim>18x18</dim>
										<tableStyle>border:solid 1px black;</tableStyle>
										<script>wizard_colorpicker.php</script>
										<JSopenParams>height=340,width=360,status=0,menubar=0,scrollbars=1</JSopenParams>
									</color>
								</wizards>
								<default></default>
								<eval>#555555</eval>
							</config>
						</TCEforms>
					</settings.graphColor>
					<settings.performanceGraphColor>
						<TCEforms>
							<label>Performance graph color</label>
							<config>
								<type>input</type>
								<size>6</size>
								<wizards>
									<color>
										<type>colorbox</type>
										<dim>18x18</dim>
										<tableStyle>border:solid 1px black;</tableStyle>
										<script>wizard_colorpicker.php</script>
										<JSopenParams>height=340,width=360,status=0,menubar=0,scrollbars=1</JSopenParams>
									</color>
								</wizards>
								<default></default>
								<eval>#555555</eval>
							</config>
						</TCEforms>
					</settings.performanceGraphColor>
				</el>
			</ROOT>
		</graph>
	</sheets>
</T3DataStructure>